{
	"name": "spec-buddy",
	"displayName": "Spec Buddy",
	"publisher": "rileymathews",
	"description": "Keeps your spec files open as you open and close source code.",
	"repository": "https://github.com/RileyMathews/spec-buddy",
	"version": "0.1.2",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/src/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "specBuddy.openSpec",
				"title": "Open Spec File"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vsce": "^1.66.0",
		"vscode-test": "^1.2.0"
	}
}
